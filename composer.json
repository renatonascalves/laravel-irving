{
  "name": "renatonascalves/laravel-irving",
  "description": "Irving support to Laravel.",
  "keywords": [
    "irving",
    "laravel",
    "headless"
  ],
  "support": {
      "issues": "https://github.com/renatonascalbes/laravel-irving/issues",
      "source": "https://github.com/renatonascalbes/laravel-irving"
    },
   "authors": [
    {
      "name": "Renato Albes",
      "email": "contato@ralv.es"
    }
  ],
  "require": {
    "php": "^7.3",
		"illuminate/filesystem": "^7.0",
		"illuminate/console": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
		"phpunit/phpunit": "^8.5.1 || ^9.0",
    "emielmolenaar/phpcs-laravel": "^1.2",
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "nunomaduro/collision": "^4.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Irving\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Irving\\IrvingServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
